<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="catel">
			<section name="logging" type="Catel.Logging.LoggingConfigurationSection, Catel.Core"/>
		</sectionGroup>
	</configSections>
	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>

	<catel>
		<logging>
			<listeners>
				<listener type="Catel.Logging.FileLogListener" FilePath="{AppData}\Logs\ErrorLog" IgnoreCatelLogging="false" IsDebugEnabled="false" IsInfoEnabled="false" IsWarningEnabled="false" IsErrorEnabled="true" MaxSizeInKiloBytes="250" TimeDisplay="DateTime"/>
				<listener type="Catel.Logging.FileLogListener" FilePath="{AppData}\Logs\WarningLog" IgnoreCatelLogging="false" IsDebugEnabled="false" IsInfoEnabled="false" IsWarningEnabled="true" IsErrorEnabled="false" MaxSizeInKiloBytes="250" TimeDisplay="DateTime"/>
				<listener type="Catel.Logging.FileLogListener" FilePath="{AppData}\Logs\InfoLog" IgnoreCatelLogging="false" IsDebugEnabled="false" IsInfoEnabled="true" IsWarningEnabled="false" IsErrorEnabled="false" MaxSizeInKiloBytes="250" TimeDisplay="DateTime"/>
			</listeners>
		</logging>
	</catel>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Numerics.Vectors" publicKeyToken="b03f5f7f11d50a3a" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-4.1.4.0" newVersion="4.1.4.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Runtime.CompilerServices.Unsafe" publicKeyToken="b03f5f7f11d50a3a" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-4.0.6.0" newVersion="4.0.6.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Buffers" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
